package entities;

public class Rectangle {

	public double width;
	public double height;
	public double area;
	public double perimeter;
	
	public double area() {
		area = (width * height);
		return area;
	}
	
	public double perimeter() {
		perimeter = (2*(width + height));
		return perimeter;
	}
	
	
	
	
}
