package entities;

public class Student {

	public String name;
	public double grade1;
	public double grade2;
	public double grade3;
	
	public double grade() {
		return (grade1 + grade2 + grade3);
	}
	
	public String verificaAprovacao() {
		if(grade() > 60.0) {
			return "PASS";
		}else {
			return "FAILED";
		}
	}
	
	public String toString() {
		return "Final Grade = " +
				String.format("%.2f", grade());
	}
}
